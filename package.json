{
  "name": "vue2webpack2",
  "version": "1.0.0",
  "description": "vue2.0 + webpack2.x",
  "main": "webpack.config.js",
  "scripts": {
    "dll": "set NODE_ENV=production&&webpack --progress --colors --config ./webpack-dll.config.js",
    "dll-dev": "webpack --progress --colors --config ./webpack-dll.config.js",
    "vendor": "set NODE_ENV=production&&webpack --progress --colors --config ./webpack-vendor.config.js",
    "vendor-dev": "webpack --progress --colors --config ./webpack-vendor.config.js",
    "deploy": "set NODE_ENV=production&&webpack --bail --progress --colors --hide-modules",
    "watch": "webpack --progress --watch",
    "build": "webpack --hide-modules",
    "pc_dll": "set PLATFORM=pc&&set NODE_ENV=production&&webpack --progress --colors --config ./webpack-dll.config.js",
    "pc_dll-dev": "set PLATFORM=pc&&webpack --progress --colors --config ./webpack-dll.config.js",
    "pc_vendor": "set PLATFORM=pc&&set NODE_ENV=production&&webpack --progress --colors --config ./webpack-vendor.config.js",
    "pc_vendor-dev": "set PLATFORM=pc&&webpack --progress --colors --config ./webpack-vendor.config.js",
    "pc_deploy": "set PLATFORM=pc&&set NODE_ENV=production&&webpack --progress --colors --hide-modules",
    "pc_watch": "set PLATFORM=pc&&webpack --progress --watch",
    "pc_build": "set PLATFORM=pc&&webpack --hide-modules",
    "browser": "browser-sync start --server --files '**/*.*'"
  },
  "keywords": [],
  "author": "smy",
  "license": "ISC",
